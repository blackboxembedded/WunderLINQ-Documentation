msgid "APP_SETTINGS_FAULT_DESC"
msgstr "Enable to turn on notifications for critical faults."

msgid "APP_SETTINGS_TIRE_TITLE"
msgstr "Custom Tire Pressure Threshold:"

msgid "APP_SETTINGS_TIRE_DESC"

msgid "APP_SETTINGS_DISPLAY_TITLE"
msgstr "Display Brightness:"

msgid "APP_SETTINGS_DISPLAY_DESC"
msgstr "Set display brightness to the max when in day mode."

msgid "APP_SETTINGS_NIGHT_TITLE"
msgstr "Night Mode:"

msgid "APP_SETTINGS_NIGHT_DESC"
msgstr "Enable to turn on the white on black mode for easier on the eyes night experience.  Auto uses the phone's ambient light sensor."

msgid "APP_SETTINGS_NIGHTDELAY_TITLE"
msgstr "Auto Night Mode Delay:"

msgid "APP_SETTINGS_NIGHTDELAY_DESC"
msgstr "How many seconds of darkness before enabling Night Mode."

msgid "APP_SETTINGS_HIDENAVBAR_TITLE"
msgstr "Hide NavBar when not in use:"

msgid "APP_SETTINGS_HIDENAVBAR_DESC"
msgstr "Auto Hide hide NavBar when not in use."

msgid "APP_SETTINGS_ALERT_TITLE"
msgstr "Alert Settings"

msgid "APP_SETTINGS_FAULT_TITLE"

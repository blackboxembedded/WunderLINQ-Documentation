msgid "APP_SETTINGS_ADVANCED_TITLE"
msgstr "Advanced Settings"

msgid "APP_SETTINGS_COMPASS_TITLE"
msgstr "GPS Bearing:"

msgid "APP_SETTINGS_COMPASS_DESC"
msgstr "Select to use GPS bearing instead on compass."

msgid "APP_SETTINGS_PPREVIEW_TITLE"
msgstr "Photo Preview:"

msgid "APP_SETTINGS_PPREVIEW_DESC"
msgstr "Displays photo after capture."

msgid "APP_SETTINGS_LAUNCH_TITLE"
msgstr "Auto Launch App:"

msgid "APP_SETTINGS_LAUNCH_DESC"
msgstr "Launches app when WunderLINQ hardware connects."

msgid "APP_SETTINGS_INTEGRATION_TITLE"
msgstr "Integration Settings"

msgid "APP_SETTINGS_INTEGRATIONS_TITLE"
msgstr "Integrations:"

msgid "APP_SETTINGS_INTEGRATIONS_DESC"
msgstr "Settings for 3rd party software and hardware integrations."

msgid "APP_SETTINGS_EXPERIMENTAL_TITLE"
msgstr "Experimental Features"

msgid "APP_SETTINGS_PIP_TITLE"
msgstr "Enable Picture in Picture:"

msgid "APP_SETTINGS_PIP_DESC"
msgstr "When enabled the Motorcycle Screen is put in a floating window when you leave the app."

msgid "APP_SETTINGS_PIPORIENTATION_TITLE"
msgstr "PiP Orientation:"

msgid "APP_SETTINGS_PIPORIENTATION_DESC"
msgstr "Select landscape or portrait layout for the PiP window."

msgid "APP_SETTINGS_PIPCELL_TITLE"
msgstr "PiP Cell Count:"

msgid "APP_SETTINGS_PIPCELL_DESC"
msgstr "Number of data cells to display in PiP window."

msgid "APP_SETTINGS_DEBUG_TITLE"
msgstr "Debug Settings"

msgid "APP_SETTINGS_LOGGING_TITLE"
msgstr "Enable Debug Logging:"

msgid "APP_SETTINGS_LOGGING_DESC"
msgstr "Only enable if requested, and should be shut off when you are done.  May cause performance issues if left on."

msgid "APP_SETTINGS_SCREENS_SCREEN_TITLE"
msgstr "Main Screen Customization"

msgid "APP_SETTINGS_DATAGRID_TITLE"
msgstr "Data Grid Customization"

msgid "APP_SETTINGS_DATAGRID_SUMMARY"
msgstr "Controls display and location of datapoints on the Motorcycle Screen.  Currently supported data points are below."

msgid "APP_SETTINGS_DATAGRID_WARNING"
msgstr "*Requires the On-board Computer Pro option on your motorcycle.  **Requires the necessary sensor in your smart phone."

msgid "APP_SETTINGS_QUICKTASK_TITLE"
msgstr "Quick Task Customization"

msgid "DATA_SUPPORTED"
msgstr "Supported Data Points"

msgid "APP_SETTINGS_QUICKTASK_SUMMARY"
msgstr ""
"Controls display and order of tasks on the Quick Task Screen.  Current task "
"options include the following."

msgid "APP_SETTINGS_QUICKTASK_NAVIGATION_TITLE"
msgstr "Navigation:"

msgid "APP_SETTINGS_QUICKTASK_NAVIGATION_FUNC"
msgstr "Launches a supported 3rd party navigation app into driving mode."

msgid "APP_SETTINGS_QUICKTASK_GOHOME_TITLE"
msgstr "Go Home:"

msgid "APP_SETTINGS_QUICKTASK_GOHOME_FUNC"
msgstr "Launches a supported 3rd party navigation app and routes you to the destination you’ve defined in the WundeLINQ App Settings."

msgid "APP_SETTINGS_QUICKTASK_PHONE_TITLE"
msgstr "Call Favorite Number:"

msgid "APP_SETTINGS_QUICKTASK_PHONE_FUNC"
msgstr "Launches your phone app and dials the phone number you’ve defined in the WundeLINQ App Settings."

msgid "APP_SETTINGS_QUICKTASK_CONTACTS_TITLE"
msgstr "Call Contact:"

msgid "APP_SETTINGS_QUICKTASK_CONTACTS_FUNC"
msgstr "Provides a list of your address book phone numbers that you can in turn call."

msgid "APP_SETTINGS_QUICKTASK_PHOTO_TITLE"
msgstr "Take Photo:"

msgid "APP_SETTINGS_QUICKTASK_PHOTO_FUNC"
msgstr "Take a picture with your rear camera."

msgid "APP_SETTINGS_QUICKTASK_SELFIE_TITLE"
msgstr "Take Selfie:"

msgid "APP_SETTINGS_QUICKTASK_SELFIE_FUNC"
msgstr "Take a picture with your front camera."

msgid "APP_SETTINGS_QUICKTASK_VIDEO_TITLE"
msgstr "Start/Stop Recording:"

msgid "APP_SETTINGS_QUICKTASK_VIDEO_FUNC"
msgstr "Controls video recording through your devices rear camera."

msgid "APP_SETTINGS_QUICKTASK_VIDEO_FRONT_TITLE"
msgstr "Start Selfie/Stop Recording:"

msgid "APP_SETTINGS_QUICKTASK_VIDEO_FRONT_FUNC"
msgstr "Controls video recording through your devices front camera."

msgid "APP_SETTINGS_QUICKTASK_TRIP_TITLE"
msgstr "Start/Stop Trip Log:"

